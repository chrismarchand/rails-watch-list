<h1>My movie lists</h1>
  <% @lists.each do |list| %>
  <div class="card">
    <%= cl_image_tag("of8ztpevdp2rubt7gbso",
      width: 400, height: 300, crop: :fill) %>      <div class="card-body">
      <p><%= link_to list.name, list_path(list) %></p>
  </div>  <% end %>

<%= link_to "Create a new list", new_list_path, class: "btn btn-primary" %>
